<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.defend.android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activites.MyIdeasActivity"></activity>
        <activity android:name=".activites.NewIdeaSecondActivity" />
        <activity android:name=".activites.NewIdeaFirstActivity" />
        <activity android:name=".activites.InfographicDetailActivity" />
        <activity android:name=".activites.BookBillingActivity" />
        <activity android:name=".activites.BookDetailActivity" />
        <activity android:name=".activites.BookCartActivity" />
        <activity android:name=".activites.WarfareDetailActivity" />
        <activity android:name=".activites.EventDetailActivity" />
        <activity android:name=".activites.NewsActivity" />
        <activity android:name=".activites.SignUpActivity" />
        <activity android:name=".activites.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activites.LoginActivity" />
        <activity
            android:name=".activites.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>